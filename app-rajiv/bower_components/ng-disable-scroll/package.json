{
  "name": "ng-disable-scroll",
  "version": "0.1.1",
  "description": "Disable page from scrolling when certain events happen.",
  "main": "disable-scroll.js",
  "scripts": {
    "build": "uglifyjs disable-scroll.js -c -m --bare-returns -o disable-scroll.min.js",
    "bump": "bump --prompt --commit --tag --push --all",
    "push": "git push --tag origin master:master",
    "release": "npm run build && npm run bump"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaketbaby/ng-disable-scroll.git"
  },
  "keywords": [
    "Angular",
    "Scroll"
  ],
  "author": "Shaketbaby",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/shaketbaby/ng-disable-scroll/issues"
  },
  "homepage": "https://github.com/shaketbaby/ng-disable-scroll",
  "devDependencies": {
    "uglify-js": "^2.4.23",
    "version-bump-prompt": "^1.3.1"
  }
}